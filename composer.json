{
    "name": "simaland/api-php-client",
    "type": "library",
    "description": "Client library for Simaland APIs",
    "keywords": ["simaland", "api", "php", "client"],
    "homepage": "https://github.com/sima-land/api-php-client",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgeny Gavrilov",
            "email": "e.r.gavrilov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "guzzlehttp/guzzle": "~6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5"
    },
    "autoload": {
        "psr-4": {
            "simaland\\apiClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "simaland\\apiClient\\tests\\": "tests"
        }
    }
}
